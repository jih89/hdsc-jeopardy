{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1.html"
    },
    {
      "src": "^/admin/?$",
      "dest": "/admin/index.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|ico|ttf))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}

